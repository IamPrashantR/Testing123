package Work;

public class Sample {

	public static void main(String[] args) {
		int[] arr= {1,3,4,3,1,3,3};
		int count=1;
		int i;
		int temp=0;
		for(i=0;i<=arr.length-1;i++)
		{
			for(int j=i+1;j<=arr.length-1;j++)
			{
				if(arr[i]==arr[j])
				{
					count++;
					arr[i]=0;
					
				}
				 
				
			}
			if(count>=2)
			{
				System.out.println("Duplicate");
				System.out.println(arr[i]+"->"+count);
				count=1;
			}
			
			
			
			
			
		}
		

	}

}
